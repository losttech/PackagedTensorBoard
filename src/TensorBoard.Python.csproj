<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>LostTech.TensorBoard.Python</AssemblyName>
    <RootNamespace>LostTech.TensorBoard</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <IncludeBuildOutput>false</IncludeBuildOutput>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>LostTech.TensorBoard.Python.runtime.win-x64</PackageId>
    <VersionPrefix>2.10.0</VersionPrefix>
    <Authors>Victor Nova</Authors>
    <Company>Lost Tech LLC</Company>
    <Product>Loss</Product>
    <Copyright>Â© Lost Tech LLC 2022</Copyright>
    <PackageProjectUrl>https://github.com/losttech/PackagedTensorBoard</PackageProjectUrl>
    <RepositoryUrl>https://github.com/losttech/PackagedTensorBoard.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>native TensorBoard</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="$(PackagedEnv)/**/*">
      <PackagePath>runtimes/win-x64/native/</PackagePath>
      <Pack>true</Pack>
    </Content>
  </ItemGroup>

</Project>
